<UserControl x:Class="Isis.CustomControls.Battery_Pack.BatteryPackVoltageCurrent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ignore="http://www.ignore.com"
             x:Name="PackVoltageCurrent"
             Width="215"
             Height="140"
             mc:Ignorable="d ignore"
             DataContext="{Binding BatteryView, Source={StaticResource Locator} }">
    <Grid>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="7,15,0,0" Height="50" Width="130" Text="{Binding BmuData.BatteryPackVA.BatteryVoltage}" FontSize="36" TextAlignment="Center" />
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="mV" VerticalAlignment="Top" Margin="142,17,0,0" FontSize="36" Width="63" />
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="7,82,0,0" Height="50" Width="130" Text="{Binding BmuData.BatteryPackVA.BatteryCurrent}" FontSize="36" TextAlignment="Center" />
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="mA" VerticalAlignment="Top" Margin="142,82,0,0" FontSize="36" Width="63" />
    </Grid>
</UserControl>